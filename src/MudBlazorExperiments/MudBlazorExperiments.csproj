<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>6.1.2</Version>
    <PackageProjectUrl>https://github.com/snakex64/MudBlazor.Experiments</PackageProjectUrl>
    <RepositoryUrl>https://github.com/snakex64/MudBlazor.Experiments</RepositoryUrl>
    <Authors />
    <RootNamespace>MudBlazor.Experiments</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.3" />
    <PackageReference Include="MudBlazor" Version="6.0.9" />
  </ItemGroup>
  
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>
  
</Project>
